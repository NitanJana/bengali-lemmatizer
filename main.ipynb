    "    for i in target_suffix:\n",
    "        count=0\n",
    "        for j in range(-1,(-(len(i)+1)),-1):\n",
    "            if (word[j]==i[j]):\n",
    "                count+=1\n",
    "            else:\n",
    "                break\n",
    "        if(count==len(i)):\n",
    "            final_suffix=i\n",
